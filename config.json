{
    "05:time": {
        "mode" : "time",
        "param" : "%d/%m/%y %H:%M:%S"
    },
    "04:asound": {
        "mode" : "asound",
    	"prefix":"V:",
        "urgent" : 100,
        "device" : "default",
	"param" : "Master",
	"exec1" : "pavucontrol&",
	"icon" : "volume%3.xbm"
    },
    "03:hwmon": {
    "prefix":"CPU:",
    "mode":"hwmon",
    "device":"/sys/devices/platform/coretemp.0/temp1_input",
        "urgent" : 85,
    "offset":1000
    },
    "02:nvidia": {
    "mode":"nvidia",
    "prefix":"GPU:",
        "urgent" : 85,
    "device":"nvidia-smi --query-gpu=temperature.gpu --format=csv,noheader"
    },
    "01:ipv4":{
    "mode":"ipv4",
    "device":"br0"
    }
}
